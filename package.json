{
  "name": "compute-intmin",
  "version": "0.0.0",
  "description": "Returns the minimum value of a specified integer type.",
  "author": {
    "name": "Athan Reines",
    "email": "kgryte@gmail.com"
  },
  "contributors": [
    {
      "name": "Athan Reines",
      "email": "kgryte@gmail.com"
    }
  ],
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "test-cov": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coverage -- -R spec",
    "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coveralls/coverage --report lcovonly -- -R spec && cat ./reports/coveralls/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./reports/coveralls"
  },
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "git://github.com/compute-io/intmin.git"
  },
  "keywords": [
    "compute.io",
    "compute",
    "computation",
    "min",
    "minimum",
    "integer",
    "int",
    "int8",
    "int16",
    "int32",
    "uint8",
    "uint16",
    "uint32",
    "const",
    "constant",
    "util",
    "utility"
  ],
  "bugs": {
    "url": "https://github.com/compute-io/intmin/issues"
  },
  "dependencies": {
    "compute-const-min-int16": "^1.0.0",
    "compute-const-min-int32": "^1.0.0",
    "compute-const-min-int8": "^1.0.0",
    "validate.io-string-primitive": "^1.0.0"
  },
  "devDependencies": {
    "chai": "3.x.x",
    "mocha": "2.x.x",
    "coveralls": "^2.11.1",
    "istanbul": "^0.3.0",
    "jshint": "2.x.x",
    "jshint-stylish": "^2.0.0"
  },
  "license": "MIT"
}
